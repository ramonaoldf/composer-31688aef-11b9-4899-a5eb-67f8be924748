{
    "name": "presttec\/laravel-breeze",
    "description": "Minimal Laravel authentication scaffolding with Blade and Tailwind.",
    "keywords": [
        "laravel",
        "auth"
    ],
    "license": "MIT",
    "support": {
        "issues": "https:\/\/github.com\/presttec\/laravel-breeze\/issues",
        "source": "https:\/\/github.com\/presttec\/laravel-breeze"
    },
    "authors": [
        {
            "name": "Ramon Oliveira",
            "email": "ramon@presttec.com.br"
        }
    ],
    "require": {
        "php": "^8.2.0",
        "presttec\/illuminate-console": "^11.0",
        "presttec\/illuminate-filesystem": "^11.0",
        "presttec\/illuminate-support": "^11.0",
        "presttec\/illuminate-validation": "^11.0",
        "presttec\/console": "^7.0"
    },
    "require-dev": {
        "presttec\/laravel-framework": "^11.0",
        "presttec\/testbench-core": "^9.0",
        "presttec\/phpstan": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Breeze\\": "src\/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravel\\Breeze\\BreezeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "replaces": {
        "laravel\/breeze": "*"
    },
    "conflicts": {
        "laravel\/breeze": "*"
    }
}