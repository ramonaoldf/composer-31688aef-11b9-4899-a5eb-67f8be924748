{
    "name": "presttec\/laravel-breeze",
    "description": "Minimal Laravel authentication scaffolding with Blade and Tailwind.",
    "keywords": [
        "laravel",
        "auth"
    ],
    "license": "MIT",
    "support": {
        "issues": "https:\/\/github.com\/presttec\/laravel-breeze\/issues",
        "source": "https:\/\/github.com\/presttec\/laravel-breeze"
    },
    "authors": [
        {
            "name": "Ramon Oliveira",
            "email": "ramon@presttec.com.br"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "presttec\/illuminate-filesystem": "^8.40",
        "presttec\/illuminate-support": "^8.40",
        "presttec\/illuminate-validation": "^8.40"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Breeze\\": "src\/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Breeze\\BreezeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "replaces": {
        "laravel\/breeze": "*"
    },
    "conflicts": {
        "laravel\/breeze": "*"
    },
    "require-dev": []
}